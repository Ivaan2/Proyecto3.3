<Window x:Class="Proyecto3._3.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Proyecto3._3"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="264"/>
            <ColumnDefinition Width="528"/>
        </Grid.ColumnDefinitions>
        <StackPanel VerticalAlignment="Top" HorizontalAlignment="Center">
            <Image Source="logoJA.png" Width="150" Margin="0,50,0,0"/>
        </StackPanel>
        <StackPanel Grid.Column="1" Orientation="Horizontal" Background="Aquamarine">
            <StackPanel VerticalAlignment="Center" Width="250" Orientation="Horizontal">
                <Label Content="Nombre" Width="50" HorizontalAlignment="Left"/>
                <TextBox Width="100" Margin="213,0" HorizontalAlignment="Right"/>
            </StackPanel>
        </StackPanel>

    </Grid>
</Window>
